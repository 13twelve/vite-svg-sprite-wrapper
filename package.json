{
  "name": "vite-svg-sprite-wrapper",
  "version": "1.0.0",
  "packageManager": "pnpm@7.5.0",
  "description": "Creating one sprite file on the fly",
  "author": "vshepel <web.shepel@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/vshepel/vite-svg-sprite-wrapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/vshepel/vite-svg-sprite-wrapper"
  },
  "bugs": "https://github.com/vshepel/vite-svg-sprite-wrapper/issues",
  "keywords": [
    "vite",
    "vite-plugin",
    "svg",
    "svg-sprite"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vite": "^2.9.0 || ^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "svg-sprite": "^2.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@antfu/ni": "^0.18.8",
    "@types/debug": "^4.1.7",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.11.14",
    "@types/picomatch": "^2.3.0",
    "@types/svg-sprite": "^0.0.34",
    "bumpp": "^8.2.1",
    "eslint": "^8.29.0",
    "rollup": "^3.7.4",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.1"
  }
}
